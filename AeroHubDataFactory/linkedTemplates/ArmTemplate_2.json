						"parameters": {
							"FileName": "[parameters('Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FileName')]",
							"FilePath": "[parameters('Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FilePath')]"
