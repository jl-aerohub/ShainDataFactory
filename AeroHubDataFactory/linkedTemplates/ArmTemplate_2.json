				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Initial_QIF_In",
							"type": "PipelineReference"
						},
						"parameters": {
							"FileName": "[parameters('InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FileName')]",
							"FilePath": "[parameters('InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FilePath')]"
						}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/initialqifin/blobs/",
					"blobPathEndsWith": ".qif",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('InitialQIFIn Trigger_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"
					]
				}
			},
			"dependsOn": []
