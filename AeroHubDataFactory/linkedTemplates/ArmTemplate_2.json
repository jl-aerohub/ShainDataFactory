		{
			"name": "[concat(parameters('factoryName'), '/Shain Initial QIF Trigger')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Initial_QIF_In_copy1",
