					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AeroHubDataFactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "AeroHubDataFactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
