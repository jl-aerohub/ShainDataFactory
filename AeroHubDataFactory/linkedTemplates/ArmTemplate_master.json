{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AeroHubDataFactory"
		},
		"AerohubStorageAccount_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AerohubStorageAccount'"
		},
		"ErrorTesting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ErrorTesting'"
		},
		"Initial_QIF_in_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Initial_QIF_in'"
		},
		"MFIN Database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MFIN Database'"
		},
		"InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"InitialQIFIn Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0fa38953-5cae-4448-b99b-2cb26dc3aa85/resourceGroups/AeroHubPlatform/providers/Microsoft.Storage/storageAccounts/aerohubblobstorage1"
		},
		"Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"Measurement_QIF_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0fa38953-5cae-4448-b99b-2cb26dc3aa85/resourceGroups/AeroHubPlatform/providers/Microsoft.Storage/storageAccounts/aerohubblobstorage1"
		},
		"Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
