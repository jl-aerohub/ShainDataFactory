		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AeroHubDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AerohubStorageAccount_connectionString": {
						"value": "[parameters('AerohubStorageAccount_connectionString')]"
					},
					"ErrorTesting_connectionString": {
						"value": "[parameters('ErrorTesting_connectionString')]"
					},
					"Initial_QIF_in_connectionString": {
						"value": "[parameters('Initial_QIF_in_connectionString')]"
					},
					"MFIN Database_connectionString": {
						"value": "[parameters('MFIN Database_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AeroHubDataFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
