				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FileName": {
						"value": "[parameters('InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FileName')]"
					},
					"InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FilePath": {
						"value": "[parameters('InitialQIFIn Trigger_properties_Initial_QIF_In_parameters_FilePath')]"
					},
					"InitialQIFIn Trigger_properties_typeProperties_scope": {
						"value": "[parameters('InitialQIFIn Trigger_properties_typeProperties_scope')]"
					},
					"Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FileName": {
						"value": "[parameters('Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FileName')]"
					},
					"Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FilePath": {
						"value": "[parameters('Measurement_QIF_Trigger_properties_Measurement_QIF_InTest_parameters_FilePath')]"
					},
					"Measurement_QIF_Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Measurement_QIF_Trigger_properties_typeProperties_scope')]"
					},
					"Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FileName": {
						"value": "[parameters('Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FileName')]"
					},
					"Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FilePath": {
						"value": "[parameters('Shain Initial QIF Trigger_properties_Initial_QIF_In_copy1_parameters_FilePath')]"
					},
					"Shain Initial QIF Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Shain Initial QIF Trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AeroHubDataFactory_ArmTemplate_1')]"
			]
		}
	]
}