{
	"name": "Initial_QIF_In",
	"properties": {
		"activities": [
			{
				"name": "Initial_QIF_In",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "initialqifin/@{replace(pipeline().parameters.FilePath,'initialqifin','')}/@{pipeline().parameters.FileName}"
					},
					{
						"name": "Destination",
						"value": "dbo.QIFDocument"
					}
				],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"additionalColumns": [
							{
								"name": "MBD",
								"value": "$$FILEPATH"
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": false,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none",
							"detectDataType": false,
							"namespaces": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['QPID']"
								},
								"sink": {
									"name": "QPID",
									"type": "Guid"
								}
							},
							{
								"source": {
									"path": "$['IDMax']"
								},
								"sink": {
									"name": "IDMax",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['MBD']"
								},
								"sink": {
									"name": "MBD",
									"type": "String"
								}
							}
						],
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "Initial_QIF_Input",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							},
							"FilePath": {
								"value": "@pipeline().parameters.FilePath",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Intial_QIF_Sink",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "Test"
			},
			"FilePath": {
				"type": "string",
				"defaultValue": "Test"
			}
		},
		"annotations": [],
		"lastPublishTime": "2022-04-05T20:34:26Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}